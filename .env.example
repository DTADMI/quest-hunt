# Supabase Configuration (local Supabase CLI example)
# Replace with your own values if different. These are printed by `supabase start`.
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=sb_publishable_ACJWlzQHlZjBrEguHvfOxg_3BJgxAaH

# Server-side only (DO NOT prefix with NEXT_PUBLIC). Optional for privileged tasks.
# Do not expose this in the browser or commit real values.
SUPABASE_SERVICE_ROLE=

# NextAuth Configuration (unused if relying solely on Supabase Auth)
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Mapbox (for maps functionality)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Optional: Analytics
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Optional: Sentry
NEXT_PUBLIC_SENTRY_DSN=

# Optional: Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_MAINTENANCE_MODE=false
